{
  "name": "landing-page",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx vite --host --port 4000",
    "build": "vite build && tsc -b",
    "lint": "biome check ./src && bun run type-check",
    "lint:fix": "biome check --write ./src",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format",
    "start": "bun run lint:fix && bunx concurrently -p {name} --names lint,typescript,vite --prefix-colors bgGreen.bold,bgBlue.bold,blue.bold \"bun run lint\" \"bun run type-check\" \"bun run dev\""
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "fflate": "^0.8.2",
    "jose": "^5.9.6",
    "katex": "^0.16.25",
    "lucide-react": "^0.545.0",
    "pako": "^2.1.0",
    "postcss-import": "16.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-katex": "^3.1.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.0.6",
    "tailwindcss-animated": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tailwindcss/postcss": "4.1.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/katex": "^0.16.7",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/react-katex": "^3.0.4",
    "@vitejs/plugin-react": "^5.0.4",
    "jsdom": "^27.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}